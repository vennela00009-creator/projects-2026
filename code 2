public class Main {
    public static void main(String[] args) {
        
        // Print a message
        System.out.println("Hello, World!");
        
        // Variables
        int a = 10;
        int b = 20;
        
        // Addition
        int sum = a + b;
        
        // Print result
        System.out.println("Sum of a and b is: " + sum);
    }
}
