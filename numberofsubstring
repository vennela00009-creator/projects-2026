class Solution:
    def numberOfSubstrings(self, s: str) -> int:
        dic={'a':0,'b':0,'c':0}
        l=0
        res=0
        for r in range(len(s)):
            dic[s[r]]+=1
            while dic['a'] and dic['b'] and dic['c']:
                res+=len(s)-r
                dic[s[l]]-=1
                l+=1
        return res
        
